{
  "root": true,
  "env": {
    "node": true,
    "jest": true
  },
  "extends": [
    "plugin:vue/recommended",
    "@vue/airbnb",
    "prettier/vue",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-param-reassign": [
      "error",
      {
        "props": true,
        "ignorePropertyModificationsFor": ["acc", "accumulator", "e", "el", "state"]
      }
    ],
    "import/prefer-default-export": 0
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "overrides": [
    {
      "files": ["**/__tests__/*.js"],
      "env": {
        "jest": true
      }
    }
  ]
}
